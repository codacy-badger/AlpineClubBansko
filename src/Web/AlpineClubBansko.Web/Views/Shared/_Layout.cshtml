@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Алпийски Клуб Банско - </title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.1/css/all.css" integrity="sha384-gfdkjb5BdAXd+lj+gudLWI+BXq4IuLW5IT+brZEZsLFm++aCMlF1V92rMkPaX4PP" crossorigin="anonymous">
    <link href="~/lib/lightgallery/dist/css/lightgallery.min.css" rel="stylesheet" />
    <link href="~/lib/lightslider/dist/css/lightslider.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header class="py-3">
        <div class="text-center">
            <h2 class="font-weight-bold text-muted">Алпийски Клуб Банско</h2>
        </div>
    </header>

    <partial name="_Navigation" />

    <partial name="_CookieConsentPartial" />

    <div class="container py-1 my-1">

        @await Html.PartialAsync("_NotificationsPartial", this.TempData["notification"])

        @RenderBody()
    </div>

    <footer class="footer text-muted bg-white py-1">
        <div class="container">
            <div class="row justify-content-center small font-italic">
                &copy; 2018 - Алпийски Клуб Банско - <a asp-area="" asp-controller="Home" asp-action="Privacy">Поверителност</a>
            </div>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
    <script src="~/js/site.js"></script>
    <script src="~/lib/lightgallery/dist/js/lightgallery-all.js"></script>
    <script src="~/lib/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.js"></script>
    <partial name="_OptionsNavScriptsPartial" />
    @RenderSection("Scripts", required: false)
</body>
</html>